{
  "name": "house-description",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/jest-css-modules",
    "collectCoverageFrom": [
      "client/**/*.{jsx}",
      "!**/node_modules/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "version": "1.0.0",
  "description": "AirCnb house-description module",
  "main": "./server/server.js",
  "scripts": {
    "test": "jest --coverage --colors",
    "start": "nodemon app.js",
    "build": "webpack -d --watch",
    "eslint": "eslint . --ext .jsx --ext .js",
    "seed": "node ./data_generation/seedData.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AirCnB/House-Description.git"
  },
  "author": "Dussadee Wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AirCnB/House-Description/issues"
  },
  "homepage": "https://github.com/AirCnB/House-Description#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.4.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "grunt": "^1.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.1",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^6.0.2",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0",
    "tsv": "^0.2.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "mongoose": "^5.2.4",
    "mongoose-auto-increment": "^5.0.1",
    "nodemon": "^1.18.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1"
  }
}
